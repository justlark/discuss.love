{
  "name": "smorgasbord",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "npm run build && wrangler dev",
    "type-check": "vue-tsc --build",
    "cf-typegen": "wrangler types",
    "build-only:dev": "vite build --mode dev",
    "build-only:prod": "vite build --mode prod",
    "build:dev": "run-p type-check \"build-only:dev {@}\" --",
    "build:prod": "run-p type-check \"build-only:prod {@}\" --",
    "deploy:dev": "npm run build:dev && wrangler deploy",
    "deploy:prod": "npm run build:prod && wrangler deploy",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@primevue/themes": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "seedrandom": "^3.0.5",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue-to-print": "^1.1.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^24.0.10",
    "@types/seedrandom": "^3.0.8",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.39",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.8.3",
    "vfonts": "^0.0.3",
    "vite": "^7.0.1",
    "vue-tsc": "^3.0.1"
  }
}
